  "dependencies": {
    "@prisma/client": "5.21.1",
    "express": "4.21.1",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/node": "22.7.6",
    "@types/supertest": "6.0.2",
    "@vitest/ui": "2.1.3",
    "prisma": "5.21.1",
    "supertest": "7.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.6.3",
    "vitest": "2.1.3"
  }
}
